    "@types/react-dom": "^18.3.0",
    "astro": "^4.7.0",
    "better-sqlite3": "^9.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "detect-browser": "^5.3.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "joi": "^17.13.0",
    "jose": "^5.2.4",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.376.0",
    "luxon": "^3.4.4",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.13",
